    "/api/Elements/World/{worldId}/Element/{elementId}": {
      "patch": {
        "tags": [
          "Elements"
        ],
        "summary": "Try to Score a Learning Element",
        "parameters": [
          {
            "name": "token",
            "in": "header",
            "description": "LMS User Token",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "elementId",
            "in": "path",
            "description": "Id of the Element in the World",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "worldId",
            "in": "path",
            "description": "Id of the World",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "requestBody": {
          "description": "Params needed for scoring uf h5p Elements",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ScoreElementParams"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/ScoreElementParams"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/ScoreElementParams"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ScoreElementResponse"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ScoreElementResponse"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ScoreElementResponse"
                }
              }
            }
          }
        }
      }
    },
