            "type": "array",
            "items": {
              "$ref": "#/components/schemas/WorldResponse"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "GradedAnswer": {
        "type": "object",
        "properties": {
          "checked": {
            "type": "boolean"
          },
          "correct": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "GradedQuestion": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "status": {
            "type": "string",
            "nullable": true
          },
          "answers": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/GradedAnswer"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
