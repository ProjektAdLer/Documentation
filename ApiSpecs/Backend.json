  "components": {
    "schemas": {
      "AnswerAdaptivityQuestionResponse": {
        "type": "object",
        "properties": {
          "elementScore": {
            "$ref": "#/components/schemas/ElementScoreResponse"
          },
          "gradedTask": {
            "$ref": "#/components/schemas/GradedTask"
          },
          "gradedQuestion": {
            "$ref": "#/components/schemas/GradedQuestion"
          }
        },
        "additionalProperties": false
      },
      "BaseElement": {
        "required": [
          "elementCategory",
          "elementFileType",
          "elementId",
          "elementName",
          "elementUuid"
        ],
        "type": "object",
        "properties": {
          "elementId": {
            "type": "integer",
            "format": "int32"
          },
          "elementUuid": {
            "minLength": 1,
            "type": "string"
          },
          "elementName": {
            "minLength": 1,
            "type": "string"
          },
          "elementCategory": {
            "minLength": 1,
            "type": "string"
          },
          "elementFileType": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CreateWorldResponse": {
        "type": "object",
        "properties": {
          "worldNameInLms": {
            "type": "string",
            "nullable": true
          },
          "worldLmsUrl": {
            "type": "string",
            "nullable": true
          },
          "world3DUrl": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ElementScoreResponse": {
        "type": "object",
        "properties": {
          "elementId": {
            "type": "integer",
            "format": "int32"
          },
          "success": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "GetAdaptivityElementDetailsResponse": {
        "type": "object",
        "properties": {
          "element": {
            "$ref": "#/components/schemas/ElementScoreResponse"
          },
          "questions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/GradedQuestion"
            },
            "nullable": true
          },
          "tasks": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/GradedTask"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "GetElementSourceResponse": {
